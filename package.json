{
  "name": "apiwatcher-desktop",
  "productName": "APIWatcher",
  "version": "1.0.0",
  "description": "Wieloplatformowa aplikacja desktopowa do monitorowania warto≈õci JSON API",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bjornskjald/apiwatcher-desktop.git"
  },
  "author": {
    "name": "Bjornskjald",
    "email": "github@bjorn.ml"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bjornskjald/apiwatcher-desktop/issues"
  },
  "build": {
    "appId": "ml.bjorn.apiwatcher.desktop",
    "files": [
      "css/",
      "js/",
      "html/",
      "fonts/",
      "icons/",
      "node_modules/",
      "main.html",
      "package.json",
      "index.js"
    ],
    "linux": {
      "category": "Utility;Network",
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/icon.ico"
    }
  },
  "homepage": "https://github.com/Bjornskjald/apiwatcher-desktop#readme",
  "dependencies": {
    "jquery": "^3.2.1",
    "superagent": "^3.6.3"
  }
}
